<AuLabel for={{this.inputId}}>
  {{@field.label}}
</AuLabel>

<div id={{this.inputId}} class="au-o-grid au-o-grid--flush">
  {{#each this.options as |option|}}
    <div
      class="au-o-grid__item
        {{if
          this.isColumnLayout
          'au-u-1-1@tiny au-u-1-2@small au-u-1-3@medium'
        }}"
    >
      <AuControlCheckbox
        @identifier="{{@field.uri}}/<{{option.subject.value}}"
        @name={{@field.uri.value}}
        @label={{option.label}}
        @value={{option.label}}
        @disabled={{true}}
        checked={{option.provided}}
      />
    </div>
  {{/each}}
</div>