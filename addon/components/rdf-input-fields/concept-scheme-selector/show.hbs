<AuLabel for={{this.inputId}}>
  {{@field.label}}
</AuLabel>
<PowerSelect
  @triggerId={{this.inputId}}
  @disabled={{true}}
  @selected={{this.selected}}
  @options={{this.options}}
  @onChange={{this.noop}}
  data-test-field-uri={{@field.uri.value}}
  as |concept|
>
  {{concept.label}}
</PowerSelect>